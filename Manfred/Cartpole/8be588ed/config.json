{
  "name": "DDQN",
  "env_id": "CartPole-v1",
  "render_mode": "rgb_array",
  "env_kwargs": {
    "sutton_barto_reward": false
  },
  "algorithm": {
    "algorithm": "DDQN",
    "learning_rate": 0.001,
    "gamma": 0.99,
    "buffer_size": 50000,
    "batch_size": 64,
    "learning_starts": 1000,
    "train_freq": 4,
    "gradient_steps": 1,
    "target_update_interval": 500,
    "exploration_fraction": 0.2,
    "exploration_initial_eps": 1.0,
    "exploration_final_eps": 0.05,
    "net": {
      "hidden_layers": [
        128
      ],
      "activation": "relu"
    }
  },
  "episodes": {
    "episodes": 3000,
    "max_steps": 500,
    "moving_average_window": 20
  },
  "visible": true
}